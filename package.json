{
  "name": "qim-wsk",
  "version": "0.0.0",
  "engines": {
    "node": ">20.0.0"
  },
  "scripts": {
    "lib-qui:build": "ng build --project lib-qui",
    "prelib-qui:pack": "npm run lib-qui:build",
    "lib-qui:pack": "npm pack ./dist/lib-qui --pack-destination ./packages",
    
    "lib-qore:build": "ng build --project lib-qore",
    "prelib-qore:pack": "npm run lib-qore:build",
    "lib-qore:pack": "npm pack ./dist/lib-qore --pack-destination ./packages",
    
    "mock:api": "json-server --watch data/db.json --port 5050",
    "mathieu": "node ./scripts",

    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc:build": "compodoc -p tsconfig.doc.json -d ./documentation/main -n 'QIM WSK Documentation'",
    "precompodoc:serve": "npm run compodoc:build",
    "compodoc:serve": "compodoc -s -d ./documentation/main"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "bootstrap": "^5.3.8",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.13",
    "@angular/cli": "^20.3.13",
    "@angular/compiler-cli": "^20.3.0",
    "@compodoc/compodoc": "^1.1.32",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "json-server": "^1.0.0-beta.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "typescript": "~5.9.2"
  }
}